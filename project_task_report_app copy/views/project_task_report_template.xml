        <field name="name">Task Report Form</field>
        <field name="model">task.task</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Task Report">
                <sheet>
                    <group>
                        <field name="employee_id" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
